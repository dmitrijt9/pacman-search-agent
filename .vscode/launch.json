  "version": "0.2.0",
  "configurations": [
    {
      "name": "Depth First Search",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/pacman.py",
      "console": "integratedTerminal",
      "args": [
        "-l",
        "tinyMaze",
        "-p",
        "SearchAgent",
        "-a",
        "fn=depthFirstSearch"
      ]
    },
    {
      "name": "Depth First Search - medium maze",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/pacman.py",
      "console": "integratedTerminal",
      "args": [
        "-l",
        "mediumMaze",
        "-p",
        "SearchAgent",
        "-a",
        "fn=depthFirstSearch"
      ]
    },
    {
      "name": "Depth First Search - big maze",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/pacman.py",
      "console": "integratedTerminal",
      "args": [
        "-l",
        "bigMaze",
        "-p",
        "SearchAgent",
        "-z",
        ".5",
        "-a",
        "fn=depthFirstSearch",
        "--frameTime",
        "0"
      ]
    },
    {
      "name": "Breadth First Search - small maze",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/pacman.py",
      "console": "integratedTerminal",
      "args": ["-l", "tinyMaze", "-p", "SearchAgent", "-a", "fn=bfs"]
    },
    {
      "name": "Breadth First Search - medium maze",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/pacman.py",
      "console": "integratedTerminal",
      "args": ["-l", "mediumMaze", "-p", "SearchAgent", "-a", "fn=bfs"]
    },
    {
      "name": "Autograder",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/autograder.py",
      "console": "integratedTerminal"
    }
  ]
}
